<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triage"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cross_bandaid"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.nurses.triage.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nurses.triage.Screen_patient"
            android:label="@string/title_activity_screen_patient" >
        </activity>
        <activity
            android:name="com.nurses.triage.Screen_condition"
            android:label="@string/title_activity_screen_condition" >
        </activity>
        <activity
            android:name="com.nurses.triage.Screen_list_patients"
            android:label="@string/title_activity_screen_list_patients" >
        </activity>
        <activity
            android:name="com.nurses.triage.Screen_list_conditions"
            android:label="@string/title_activity_screen_list_conditions" >
        </activity>
        <activity
            android:name=".Screen_change_time"
            android:label="@string/title_activity_screen_change_time" >
        </activity>
    </application>

</manifest>
